                    </ScrollViewer>

                    <Button x:Name="Celestia_AcceptEULA"
                            Content="I Accept The EULA"
                            VerticalAlignment="Bottom"
                            Background="#7F191919"
                            BorderBrush="Transparent"
                            Foreground="White"
                            FontFamily="/Assets/Fonts/#Poppins"
                            FontSize="12"
                            Height="30"
                            Padding="0"
                            BorderThickness="0"
                            HorizontalAlignment="Right"
                            Width="143"
                            ToolTipService.InitialShowDelay="500"
                            Margin="0,0,10,10"
                            Click="Celestia_AcceptEULA_Click" />
                    <Button x:Name="Celestia_DeclineEULA"
                            Content="I Do Not Accept The EULA"
                            VerticalAlignment="Bottom"
                            Background="#7F191919"
                            BorderBrush="Transparent"
                            Foreground="White"
                            FontFamily="/Assets/Fonts/#Poppins"
                            FontSize="12"
                            Height="30"
                            Padding="0"
                            BorderThickness="0"
                            HorizontalAlignment="Left"
                            Width="183"
                            ToolTipService.InitialShowDelay="500"
                            Margin="10,0,0,10"
                            Click="Celestia_DeclineEULA_Click" />

                </Grid>
                <Grid Width="0"
                      Name="Celestia_Customise">
                    <TextBlock Text="Customisation"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Margin="20,50,0,0"
                               FontFamily="/Assets/Fonts/#Poppins Medium"
                               Foreground="#DDFFFFFF"
                               FontSize="13.5"
                               Padding="0" />
                    <TextBlock Text="Enable Or Disable Features During Installation"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Margin="20,75,0,0"
                               FontFamily="/Assets/Fonts/#Atkinson Hyperlegible"
                               Foreground="#CEDCDCDC"
                               FontSize="12"
                               Padding="0" />

                    <TextBlock Text="Installation Path"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Margin="55,120,0,0"
                               FontFamily="/Assets/Fonts/#Poppins"
                               Foreground="#DEDFDFDF"
                               FontSize="12"
                               Padding="0" />

                    <Border Height="40"
                            Margin="55 145 55 0"
                            VerticalAlignment="Top"
                            BorderBrush="#3FC1C1C1"
                            Background="#3f191919"
                            BorderThickness="1.2"
                            CornerRadius="6">
                        <Grid>

                            <TextBlock x:Name="Celestia_InstallationPath"
                                       Text="C:\Users\hunt3r\AppData\Local\Nebula Softworks\Nebula Client\Applications\Celestia IDE"
                                       Foreground="White"
                                       VerticalAlignment="Center"
                                       Padding="10 0 10 0" />

                            <Button x:Name="Celestia_InstallationPathSelectorButton"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    BorderBrush="Transparent"
                                    Foreground="#AAAAAA"
                                    Width="30"
                                    Height="30"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Right"
                                    Margin="0 0 6 0"
                                    FontFamily="/Installer;component/Assets/Fonts/Icon/#Font Awesome 6 Free Solid"
                                    HorizontalContentAlignment="Center"
                                    VerticalContentAlignment="Center"
                                    Padding="0"
                                    FontSize="12"
                                    Content="" />

                        </Grid>
                    </Border>

                    <TextBlock Text="Launcher Installation Path"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Margin="55,195,0,0"
                               FontFamily="/Assets/Fonts/#Poppins"
                               Foreground="#DEDFDFDF"
                               FontSize="12"
                               Padding="0" />

                    <Border Height="40"
                            Margin="55 220 55 0"
