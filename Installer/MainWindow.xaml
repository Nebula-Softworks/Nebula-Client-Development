                                                                        BeginTime="0:0:12.4"
                                                                        From="0,0,180,180"
                                                                        To="0,0,160,160"
                                                                        Duration="0:0:0.9">
                                                        <ThicknessAnimation.EasingFunction>
                                                            <PowerEase EasingMode="EaseInOut"
                                                                       Power="5" />
                                                        </ThicknessAnimation.EasingFunction>
                                                    </ThicknessAnimation>
                                                    <ThicknessAnimation Storyboard.TargetProperty="Margin"
                                                                        BeginTime="0:0:13.3"
                                                                        From="0,0,160,160"
                                                                        To="0,160,160,0"
                                                                        Duration="0:0:2.1">
                                                        <ThicknessAnimation.EasingFunction>
                                                            <PowerEase EasingMode="EaseInOut"
                                                                       Power="3.2" />
                                                        </ThicknessAnimation.EasingFunction>
                                                    </ThicknessAnimation>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </DataTrigger.EnterActions>
                                        <DataTrigger.ExitActions>
                                            <StopStoryboard BeginStoryboardName="SpinStoryboard" />
                                        </DataTrigger.ExitActions>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Image.Style>
                    </Image>

