<Window x:Class="Installer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialdesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:Installer"
        mc:Ignorable="d"
        Title="Nebula Client Installer"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        ResizeMode="NoResize"
        Loaded="Window_Loaded"
        AllowsTransparency="True"
        SnapsToDevicePixels="True"
        Background="Transparent"
        materialdesign:ButtonAssist.CornerRadius="6"
        FontFamily="/Installer;component/Assets/Fonts/#Poppins"
        Icon="Nebula Client Installer Icon.ico"
        Height="490"
        Width="850">
    <WindowChrome.WindowChrome>
        <WindowChrome GlassFrameThickness="0"
                      ResizeBorderThickness="2" />
    </WindowChrome.WindowChrome>

    <Border x:Name="MainBorder"
            Margin="15"
            BorderBrush="#307F7F7F"
            BorderThickness="1.2"
            Background="#0A0A0A"
            CornerRadius="10">
        <Border.Effect>
            <DropShadowEffect BlurRadius="15"
                              ShadowDepth="3"
                              RenderingBias="Quality"
                              Opacity="0.8"
                              Direction="270"
                              Color="#1b1b1b" />
        </Border.Effect>
        <Grid>

            <Grid x:Name="Wallpapers"
                  ClipToBounds="True">
                <Grid.Effect>
                    <BlurEffect Radius="10"
                                KernelType="Gaussian"
                                RenderingBias="Quality" />
                </Grid.Effect>
                <Grid.Clip>
                    <RectangleGeometry Rect="0,0,818,458"
                                       RadiusX="10"
                                       RadiusY="10" />
                </Grid.Clip>
                <Border x:Name="Wallpaper1"
                        Opacity="0.501"
                        Margin="-5"
                        CornerRadius="8">
                    <Border.Background>
                        <ImageBrush ImageSource="/Installer;component/Assets/Graphics/Nebula Client Installer Wallpapers/1.png"
                                    Stretch="UniformToFill" />
                    </Border.Background>
                </Border>
                <Border x:Name="Wallpaper2"
                        Opacity="0"
                        Margin="-5"
                        CornerRadius="8">
                    <Border.Background>
                        <ImageBrush ImageSource="/Installer;component/Assets/Graphics/Nebula Client Installer Wallpapers/8.jpeg"
                                    Stretch="UniformToFill" />
                    </Border.Background>
                </Border>
                <Border x:Name="Wallpaper3"
                        Opacity="0"
                        CornerRadius="8">
                    <Border.Background>
                        <ImageBrush ImageSource="/Installer;component/Assets/Graphics/Nebula Client Installer Wallpapers/2.png"
                                    Stretch="UniformToFill" />
                    </Border.Background>
                </Border>
                <Border x:Name="Wallpaper4"
                        Opacity="0"
                        CornerRadius="8">
                    <Border.Background>
                        <ImageBrush ImageSource="/Installer;component/Assets/Graphics/Nebula Client Installer Wallpapers/3.png"
                                    Stretch="UniformToFill" />
                    </Border.Background>
                </Border>
                <Border x:Name="Wallpaper5"
                        Opacity="0"
                        Margin="-5"
                        CornerRadius="8">
                    <Border.Background>
                        <ImageBrush ImageSource="/Installer;component/Assets/Graphics/Nebula Client Installer Wallpapers/4.jpg"
                                    Stretch="UniformToFill" />
                    </Border.Background>
                </Border>
                <Border x:Name="Wallpaper6"
                        Opacity="0"
                        Margin="-5"
                        CornerRadius="8">
                    <Border.Background>
                        <ImageBrush ImageSource="/Installer;component/Assets/Graphics/Nebula Client Installer Wallpapers/5.png"
                                    Stretch="UniformToFill" />
                    </Border.Background>
                </Border>
                <Border x:Name="Wallpaper7"
                        Opacity="0"
                        Margin="-5"
                        CornerRadius="8">
                    <Border.Background>
                        <ImageBrush ImageSource="/Installer;component/Assets/Graphics/Nebula Client Installer Wallpapers/7.png"
                                    Stretch="UniformToFill" />
                    </Border.Background>
                </Border>
                <Border x:Name="Wallpaper8"
                        Opacity="0"
                        Margin="-5"
                        CornerRadius="8">
                    <Border.Background>
                        <ImageBrush ImageSource="/Installer;component/Assets/Graphics/Nebula Client Installer Wallpapers/6.png"
                                    Stretch="UniformToFill" />
                    </Border.Background>
                </Border>
            </Grid>

            <Border x:Name="Topbar"
                    Background="Transparent"
                    Height="40"
                    MouseLeftButtonDown="Topbar_MouseLeftButtonDown"
                    VerticalAlignment="Top">
                <Grid>
                    <TextBlock Text="Nebula Client Installer"
                               FontSize="13"
                               FontFamily="Segoe UI"
                               Foreground="#CEDFDFDF"
                               VerticalAlignment="Center"
                               HorizontalAlignment="Left"
                               Margin="12 7 5 5"
                               IsHitTestVisible="False" />
                    <Button Background="Transparent"
                            BorderThickness="0"
                            BorderBrush="Transparent"
                            Foreground="#AAAAAA"
                            Width="30"
                            Height="30"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Right"
                            Margin="0 0 5 0"
                            FontFamily="Segoe MDL2 Assets"
                            HorizontalContentAlignment="Center"
                            VerticalContentAlignment="Center"
                            Padding="0"
                            FontSize="10"
                            Content="&#xE8BB;"
                            Click="Close" />
                    <Button Background="Transparent"
                            BorderThickness="0"
                            BorderBrush="Transparent"
                            Foreground="#AAAAAA"
                            Width="30"
                            Height="30"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Right"
                            Margin="0 0 40 0"
                            FontFamily="/Installer;component/Assets/Fonts/Icon/#Font Awesome 6 Free Solid"
                            HorizontalContentAlignment="Center"
                            VerticalContentAlignment="Center"
                            Padding="0"
                            FontSize="11"
                            Content="trash"
                            ToolTip="Uninstaller"
                            Click="uninstallstart" />
                    <Button Background="Transparent"
                            BorderThickness="0"
                            BorderBrush="Transparent"
                            Foreground="#AAAAAA"
                            Width="30"
                            Height="30"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Right"
                            Margin="0 0 75 0"
                            FontFamily="/Installer;component/Assets/Fonts/Icon/#Font Awesome 6 Brands Regular"
                            HorizontalContentAlignment="Center"
                            VerticalContentAlignment="Center"
                            Padding="0"
                            FontSize="11"
                            Content=""
                            Click="discordjoin" />
                </Grid>
            </Border>

            <Grid Visibility="Hidden"
                  x:Name="Loader"
                  IsHitTestVisible="True">
                <StackPanel x:Name="loaderstack"
                            VerticalAlignment="Center">
                    <StackPanel.RenderTransform>
                        <ScaleTransform CenterX="409"
                                        CenterY="19.425"
                                        ScaleX="1"
                                        ScaleY="1" />
                    </StackPanel.RenderTransform>
                    <TextBlock Text="Loading Installer.."
                               FontSize="15"
                               FontFamily="/Installer;component/Assets/Fonts/#Poppins"
                               Foreground="#CEDFDFDF"
                               HorizontalAlignment="Center"
                               Margin="0 0 0 12" />
                    <Grid Width="300"
                          ClipToBounds="False">
                        <Border Background="#80AbAbAb"
                                Height="3"
                                CornerRadius="3">
                        </Border>
                        <Border x:Name="ProgressBarFill"
                                Width="20"
                                Height="3.5"
                                Margin="0 0.85 0 0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Background="White"
                                CornerRadius="3" />
                    </Grid>
                </StackPanel>
            </Grid>

            <Grid x:Name="Home"
                  Visibility="Collapsed">
                <StackPanel x:Name="homestack"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Width="400">
                    <TextBlock Text="Welcome to Nebula Client"
                               FontSize="16"
                               Foreground="#DFDFDF"
                               HorizontalAlignment="Center"
                               FontWeight="Medium"
                               Margin="0 0 0 6" />
                    <TextBlock Text="Select what you would like to do with the Installer."
                               FontSize="13.5"
                               Foreground="#CEDFDFDF"
                               HorizontalAlignment="Center"
                               FontFamily="/Installer;component/Assets/Fonts/#Poppins Light"
                               Margin="0 0 0 24" />

                    <Button Background="#7F191919"
                            BorderThickness="0"
                            ToolTipService.InitialShowDelay="500"
                            BorderBrush="Transparent"
                            Foreground="#AAAAAA"
                            Height="60"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Stretch"
                            Margin="0 0 0 5"
                            HorizontalContentAlignment="Stretch"
                            VerticalContentAlignment="Stretch"
                            Padding="10 10 10 10"
                            FontSize="10"
                            Click="celestiastart">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="40" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Image Source="/Assets/Graphics/Celestia IDE No Background.png"
                                   RenderOptions.BitmapScalingMode="Fant"
                                   Height="34"
                                   Grid.RowSpan="2"
                                   Margin="0 0 6 0" />
                            <TextBlock Text="Install Celestia IDE"
                                       Margin="4 0 0 0"
                                       FontSize="14"
                                       Padding="0 0 0 3"
                                       Grid.Column="1"
                                       Foreground="#DFDFDF" />
                            <TextBlock Margin="4 2 0 0"
                                       FontSize="10"
                                       FontFamily="/Installer;component/Assets/Fonts/#Poppins Light"
                                       Grid.Column="1"
                                       Padding="0 0 0 0"
                                       Grid.Row="1"
                                       Foreground="#CEDFDFDF">
                                <Run Text="The" />
                                <Run Foreground="White"
                                     Text="Perfect" />
                                <Run Text="Lua Scripting IDE. Modern, And Classics Reimagined." />
                            </TextBlock>
                            <Label Background="Transparent"
                                   BorderThickness="0"
                                   BorderBrush="Transparent"
                                   Foreground="#DDDDDD"
                                   Width="14"
                                   Height="30"
                                   VerticalAlignment="Center"
                                   HorizontalAlignment="Right"
                                   Margin="0 0 0 0"
                                   FontFamily="Segoe MDL2 Assets"
                                   HorizontalContentAlignment="Center"
                                   VerticalContentAlignment="Center"
                                   Padding="0"
                                   FontSize="10"
                                   Grid.RowSpan="2"
                                   Grid.Column="1"
                                   Content="&#xE76C;" />
                        </Grid>
                    </Button>

                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition Width="186" />
                        </Grid.ColumnDefinitions>

                        <Button Background="#7F191919"
                                BorderThickness="0"
                                ToolTipService.InitialShowDelay="500"
                                BorderBrush="Transparent"
                                Foreground="#AAAAAA"
                                Height="55"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Stretch"
                                Margin="0 0 3 3"
                                FontFamily="/Installer;component/Assets/Fonts/#Poppins"
                                HorizontalContentAlignment="Left"
                                VerticalContentAlignment="Center"
                                Padding="10"
                                FontSize="10"
                                Click="nebulatrinityenginestart">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="29" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Image Source="/Assets/Graphics/Nebula Trinity Engine No Background.png"
                                       RenderOptions.BitmapScalingMode="Fant"
                                       Height="34"
                                       Grid.RowSpan="2"
                                       Margin="0 0 0 0" />
                                <TextBlock Text="Nebula Trinity Engine"
                                           Margin="4 -1 0 2"
                                           FontSize="13"
                                           Padding="0 0 0 3"
                                           Grid.Column="1"
                                           Foreground="#DFDFDF" />
                                <TextBlock Margin="4 2 0 0"
                                           FontSize="9"
                                           FontFamily="/Installer;component/Assets/Fonts/#Poppins Light"
                                           Grid.Column="1"
                                           Padding="0 0 0 0"
                                           Grid.Row="1"
                                           Foreground="#CEDFDFDF">
                                    <Run Text="Roblox Scripting Engine." />
                                    <Run Foreground="White"
                                         Text="Unbound." />
                                </TextBlock>
                            </Grid>
                        </Button>
                        <Button Background="#7F191919"
                                BorderThickness="0"
                                ToolTipService.InitialShowDelay="500"
                                BorderBrush="Transparent"
                                Foreground="#AAAAAA"
                                Height="55"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Stretch"
                                Margin="3 0 0 3"
                                FontFamily="/Installer;component/Assets/Fonts/#Poppins"
                                HorizontalContentAlignment="Left"
                                VerticalContentAlignment="Center"
                                Padding="10"
                                FontSize="10"
                                Grid.Column="1"
                                Click="nebulalibrarystart">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="29" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Image Source="/Assets/Graphics/Celestia IDE No Background.png"
                                       RenderOptions.BitmapScalingMode="Fant"
                                       Height="34"
                                       Grid.RowSpan="2"
                                       Margin="0 0 0 0" />
                                <TextBlock Text="Nebula Library"
                                           Margin="4 -1 0 2"
                                           FontSize="13"
                                           Padding="0 0 0 3"
                                           Grid.Column="1"
                                           Foreground="#DFDFDF" />
                                <TextBlock Margin="4 2 0 0"
                                           FontSize="9"
                                           FontFamily="/Installer;component/Assets/Fonts/#Poppins Light"
                                           Grid.Column="1"
                                           Padding="0 0 0 0"
                                           Grid.Row="1"
                                           Foreground="#CEDFDFDF">
                                    Your Utilities. All In One.
                                </TextBlock>
                            </Grid>
                        </Button>
                    </Grid>

                </StackPanel>
                <TextBlock FontSize="8.5"
                           Foreground="#CEDFDFDF"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           Margin="0 230.5 0 0"
                           Text="Wish To Uninstall? Find It In The Control Bar." />
            </Grid>

            <StackPanel x:Name="CelestiaInstaller"
                        Orientation="Horizontal"
                        Visibility="Collapsed">

                <Grid Width="0"
                      Name="Celestia_Start">
                    <Grid x:Name="Celestia_LogoGrid"
                          HorizontalAlignment="Center"
                          Height="100"
                          Margin="0 0 0 70"
                          VerticalAlignment="Center"
                          Width="100">
                        <Image x:Name="Celestia_i1"
                               Source="/Assets/Graphics/Celestia IDE No Background.png"
                               Margin="0"
                               RenderOptions.BitmapScalingMode="HighQuality" />
                    </Grid>
                    <TextBlock Text="The Installer will begin installing the Celestia IDE, press &quot;Next&quot; to Continue."
                               HorizontalAlignment="Center"
                               VerticalAlignment="Bottom"
                               Margin="10,0,10,175"
                               FontFamily="/Assets/Fonts/#Poppins"
                               Foreground="#DDFFFFFF"
                               FontSize="13.5"
                               Padding="0" />
                    <TextBlock Text="Did you know, Celestia and the rest of Nebula Client is open sourced! You can view it on our GitHub!"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Bottom"
                               Margin="10,0,10,157" 
                               TextAlignment="Center"
                               FontFamily="/Assets/Fonts/#Atkinson Hyperlegible"
                               Foreground="#CEDCDCDC"
                               FontSize="14"
                               Padding="0" />
                    <Button x:Name="Celestia_ViewOnGithubButton"
                            Content="View on GitHub"
                            VerticalAlignment="Bottom"
                            HorizontalAlignment="Center"
                            Background="#9F171717"
                            BorderBrush="Transparent"
                            Foreground="White"
                            FontFamily="/Assets/Fonts/#Atkinson Hyperlegible"
                            FontSize="12"
                            Height="35"
                            Padding="20 10"
                            BorderThickness="0,0,0,0"
                            ToolTipService.InitialShowDelay="500"
                            Margin="0,0,0,107"
                            Click="Celestia_ViewOnGithubButton_Click" />
                    <Button x:Name="Celestia_StartProcess"
                            Content="Next"
                            VerticalAlignment="Bottom"
                            Background="#7F191919"
                            BorderBrush="Transparent"
                            Foreground="White"
                            FontFamily="/Assets/Fonts/#Poppins"
                            FontSize="12"
                            Height="30"
                            Padding="0"
                            BorderThickness="0"
                            HorizontalAlignment="Right"
                            Width="103"
                            ToolTipService.InitialShowDelay="500"
                            Margin="0,0,10,10"
                            Click="Celestia_StartProcess_Click" />

                </Grid>
                <Grid Width="0"
                      Name="Celestia_Eula">
                    <TextBlock Text="Celestia IDE EULA"
                               HorizontalAlignment="Left"
                               VerticalAlignment="Top"
                               Margin="20,50,0,0"
                               FontFamily="/Assets/Fonts/#Poppins Medium"
                               Foreground="#DDFFFFFF"
                               FontSize="14"
                               Padding="0" />

                    <ScrollViewer HorizontalAlignment="Stretch"
                                  VerticalAlignment="Stretch"
                                  HorizontalScrollBarVisibility="Auto"
                                  Margin="40,85,40,60"
                                  materialdesign:ScrollBarAssist.ButtonsVisibility="Collapsed">
                        <TextBlock x:Name="Celestia_EulaText"
                                   FontFamily="/Assets/Fonts/#Atkinson Hyperlegible"
                                   Foreground="#CEDCDCDC"
                                   FontSize="12"
                                   HorizontalAlignment="Left"
                                   Padding="0"><Run Text="Lorem Ipsum Dolor Sit Amet. " /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><LineBreak /><Run Text="Lorem Ipsum Dolor Sit Amet. Lorem Ipsum Dolor Sit Amet. " /><Run Text="Lorem Ipsum Dolor Sit Amet. " /><Run FontFamily="/Assets/Fonts/#Atkinson Hyperlegible"
