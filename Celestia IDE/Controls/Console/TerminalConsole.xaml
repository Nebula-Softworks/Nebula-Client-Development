<UserControl x:Class="Celestia_IDE.Controls.Console.TerminalConsole"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Celestia_IDE.Controls.Console"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Label x:Name="inactive"
           Foreground="#d5d3d7"
           FontSize="14"
           VerticalAlignment="Center"
           HorizontalAlignment="Center"
           VerticalContentAlignment="Center"
           HorizontalContentAlignment="Center"
           FontFamily="/Celestia IDE;component/Assets/Fonts/#Poppins"
           Content="The Integrated Terminal is coming soon."
           IsHitTestVisible="False" />
</UserControl>
